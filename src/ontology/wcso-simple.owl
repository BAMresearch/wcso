    <!-- https://w3id.org/wcso/WCSO_0000018 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000018">
        <obo:IAO_0000119>Adopted from ISO/IEC 23094-2:2021(en), 3.38</obo:IAO_0000119>
