    <!-- https://w3id.org/wcso/WCSO_0000000 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000000">
        <obo:IAO_0000119>Adopted from ISO 21253-2:2019(en), 3.11</obo:IAO_0000119>
        <rdfs:label xml:lang="de">Erhalten</rdfs:label>
        <rdfs:label xml:lang="en">yield</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.allotrope.org/ontologies/quality#AFQ_0000227"/>
        <skos:definition xml:lang="de">Das Ergeben ist eine extensive Qualität, die das Material beschreibt, das bei bzw. nach einem Prozess oder Synthese-Schritt hergestellt bzw. erhalten wurde.</skos:definition>
        <skos:definition xml:lang="en">The yield is an extensive quality describing the material that is produced by or obtained after a process or synthesis step.</skos:definition>
        <skos:example xml:lang="de">Die Synthese gab (&quot;yielded&quot;) das Produkt als weißes Pulver.</skos:example>
        <skos:example xml:lang="en">The synthesis yielded the product as a white powder.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000001 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000001">
        <rdfs:subClassOf rdf:resource="https://w3id.org/wcso/WCSO_0000000"/>
        <obo:IAO_0000119>Adopted from ISO 1213-1:2020</obo:IAO_0000119>
        <rdfs:label xml:lang="de">Ausbeute (Prozent)</rdfs:label>
        <rdfs:label xml:lang="en">yield percentage</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/CHMO_0002855"/>
        <skos:definition xml:lang="de">Die Ausbeute (in Prozent) ist die Menge eines Produkts das aus einem Produktionsprozess erhalten wurde, ausgedrückt in Prozent der (stöchiometrisch) am stärksten limitierenden Größe.</skos:definition>
        <skos:definition xml:lang="en">The yield percentage is the amount of a product that was obtained from a production process, expressed as a percentage of the (stoichiometrically) most limiting component.</skos:definition>
        <skos:example xml:lang="de">Die Synthese ergab das Produkt als weißes Pulver mit einer Ausbeute von 75%.</skos:example>
        <skos:example xml:lang="en">The synthesis yielded the product as a white powder (75%).</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000002 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000002">
        <rdfs:subClassOf rdf:resource="https://w3id.org/wcso/WCSO_0000000"/>
        <obo:IAO_0000119>In analogy to yield percentage</obo:IAO_0000119>
        <rdfs:label xml:lang="de">Ausbeute (Masse)</rdfs:label>
        <rdfs:label xml:lang="en">yield mass</rdfs:label>
        <skos:definition xml:lang="de">Die Ausbeute (als Masse) beschreibt die Masse eines Produkts das aus einem Produktionsprozess erhalten wurde.</skos:definition>
        <skos:definition xml:lang="en">The yield mass is a yield denoting the mass of a product that was obtained from a production process.</skos:definition>
        <skos:example xml:lang="de">Die Synthese ergab das Produkt als weißes Pulver mit einer Ausbeute von 100 mg.</skos:example>
        <skos:example xml:lang="en">The synthesis yielded the product as a white powder (100 mg).</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000003 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000003">
        <rdfs:label xml:lang="de">Entfernen</rdfs:label>
        <rdfs:label xml:lang="en">remove</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.allotrope.org/ontologies/function#AFFN_0000108"/>
        <skos:definition xml:lang="de">Der Vorgang, bei dem einer oder mehrere Teile einer Mischung entfernt werden, wobei mindestens ein Teil der Mischung zurückbleibt.</skos:definition>
        <skos:definition xml:lang="en">Removing is a process that denotes the act of removing one or more constituents from a mixture, leaving at least one other constituent behind.</skos:definition>
        <skos:example xml:lang="de">Das Lösungsmittel wurde im Vakuum entfernt.</skos:example>
        <skos:example xml:lang="en">The solvent was removed in vacuo.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000004 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000004">
        <rdfs:label xml:lang="de">Schallbehandlung</rdfs:label>
        <rdfs:label xml:lang="en">sonication</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://champ-project.org/images/ontology/cao.owl#CAO_000195"/>
        <rdfs:seeAlso rdf:resource="http://purl.allotrope.org/ontologies/process#AFP_0002293"/>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/CHMO_0001707"/>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/NCIT_C81871"/>
        <skos:definition xml:lang="de">Eine Technik, bei der Schall eingesetzt wird um die Komponenten einer Probe zu agitieren oder zu verteilen.</skos:definition>
        <skos:definition xml:lang="en">Sonication is a process describing a technique that uses sound energy to agitate or disrupt the components of a sample.</skos:definition>
        <skos:example xml:lang="de">Die Partikel wurden durch Schallbehandlung der Mischung dispergiert.</skos:example>
        <skos:example xml:lang="en">Sonication of the mixture resulted in the dispersion of the WCSOmaterial.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000005 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000005">
        <obo:IAO_0000119 xml:lang="en">Adopted from OBI_0302886</obo:IAO_0000119>
        <rdfs:comment>Adopted from OBI_0302886:  Centrifugation is a process separating molecules by size or density using centrifugal forces generated by a spinning rotor. G-forces of several hundred thousand times gravity are generated in ultracentrifugation</rdfs:comment>
        <rdfs:label xml:lang="de">Zentrifugieren</rdfs:label>
        <rdfs:label xml:lang="en">centrifuge</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/OBI_0302886"/>
        <skos:definition xml:lang="en">A process for separating components of a mixture by density using centrifugal forces generated by a spinning rotor.</skos:definition>
        <skos:definition xml:lang="de">Ein Prozess zur Trennung von Komponenten einer Mischung gemäß ihrer Dichte durch den Einfluss von Zentrifugalkräften eines sich drehenden Rotors.</skos:definition>
        <skos:example xml:lang="de">Die WCSOpartikel wurden aus der Dispersion durch Zentrifugation abgetrennt.</skos:example>
        <skos:example xml:lang="en">The WCSOparticles were Adopted from ISOlated from the dispersion using centrifugation.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000006 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000006">
        <obo:IAO_0000119 rdf:resource="http://purl.obolibrary.org/obo/CHMO_0002772"/>
        <rdfs:label xml:lang="de">Entgasen</rdfs:label>
        <rdfs:label xml:lang="en">degas</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/CHMO_0002772"/>
        <skos:definition xml:lang="en">A preparative step involving the removal of dissolved gases from a sample.</skos:definition>
        <skos:definition xml:lang="de">Ein präparativer Schritt, bei dem gelöstes Gas aus einer Probe entfernt wird.</skos:definition>
        <skos:example xml:lang="de">Das Lösungsmittel wurde im Vakuum entgast.</skos:example>
        <skos:example xml:lang="en">The solvent was degassed in vacuo.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000007 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000007">
        <obo:IAO_0000119>Adopted from ISO 11074:2015, 6.4.30</obo:IAO_0000119>
        <obo:IAO_0000119>Adopted from ISO 23044:2020(en), 3.1.11</obo:IAO_0000119>
        <rdfs:label xml:lang="de">Ausfällen</rdfs:label>
        <rdfs:label xml:lang="en">precipitate</rdfs:label>
        <skos:definition xml:lang="en">A process that involves the formation of separable solid products from a liquid solution or dispersion.</skos:definition>
        <skos:definition xml:lang="de">Ein Prozess, bei dem abtrennbare Feststoffe aus einer flüssigen Lösung oder Dispersion erhalten werden.</skos:definition>
        <skos:example xml:lang="de">Die WCSOpartikel wurden durch die Zugabe von Ethanol ausgefällt.</skos:example>
        <skos:example xml:lang="en">The WCSOparticles were precipitated by adding Ethanol.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000008 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000008">
        <rdfs:label xml:lang="de">Waschen</rdfs:label>
        <rdfs:label xml:lang="en">wash</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/OBI_0302888"/>
        <skos:definition xml:lang="de">Ein Prozess, bei dem ein Material das als Verunreinigung aftritt unter Anwendung einer oder mehrerer Zyklen einer Lösung entfernt wird.</skos:definition>
        <skos:definition xml:lang="en">Washing is a process by which a material entity acting as contaminant is removed by application of one or more cycles of solution.</skos:definition>
        <skos:example xml:lang="de">Die WCSOpartikel wurden drei Mal mit Ethanol gewaschen um überschüssige Ausgangsmateriualien zu entfernen.</skos:example>
        <skos:example xml:lang="en">The WCSOparticles were washed three times with water to remove excess starting materials.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000009 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000009">
        <rdfs:subClassOf rdf:resource="https://w3id.org/wcso/WCSO_0000011"/>
        <obo:IAO_0000119 rdf:resource="http://purl.obolibrary.org/obo/CHMO_0001474"/>
        <rdfs:label xml:lang="de">Abkühlen</rdfs:label>
        <rdfs:label xml:lang="en">cool</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/CHMO_0001474"/>
        <skos:definition xml:lang="de">Der Transfer von Wärmeenergie von einer Probe in die Umegbung.</skos:definition>
        <skos:definition xml:lang="en">The transfer of thermal energy from a sample to its surrounding environment.</skos:definition>
        <skos:example xml:lang="de">Die Lösung wurde in einem Eisbad auf 0°C abgekühlt.</skos:example>
        <skos:example xml:lang="en">The solution was cooled to 0°C with an ice bath.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000010 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000010">
        <obo:IAO_0000119>Adopted from ISO 28401:2024</obo:IAO_0000119>
        <rdfs:label xml:lang="de">Stoppen (einer chemischen Reaktion)</rdfs:label>
        <rdfs:label xml:lang="en">quench</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/CHMO_0002917"/>
        <skos:definition xml:lang="en">An action that results in stopping or significantly slowing down a physical process or a chemical reaction.</skos:definition>
        <skos:definition xml:lang="de">Eine Technik, bei der ein physikalischer Prozess oder eine chemische Reaktion angehalten oder signifikant verlangsamt wird.</skos:definition>
        <skos:example xml:lang="de">Die Reaktion wurde durch Wasserzugabe gestoppt.</skos:example>
        <skos:example xml:lang="en">The reaction was quenched through the addition of water.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000011 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000011">
        <obo:IAO_0000119 rdf:resource="http://purl.obolibrary.org/obo/CHMO_0002770"/>
        <rdfs:label xml:lang="de">Aufheizen</rdfs:label>
        <rdfs:label xml:lang="en">heat</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/CHMO_0002770"/>
        <skos:definition xml:lang="en">A sample preparation step involving the increase in temperature of a sample.</skos:definition>
        <skos:definition xml:lang="de">Ein präparativer Schritt, bei dem die Temperatur einer Probe erhöht wird.</skos:definition>
        <skos:example xml:lang="de">Die Probe wurde auf 100°C geheizt.</skos:example>
        <skos:example xml:lang="en">The sample was heated to 100°C.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000012 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000012">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000057"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/wcso/WCSO_0000027"/>
                            <rdf:Description rdf:about="https://w3id.org/wcso/WCSO_0000028"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="de">Rühren</rdfs:label>
        <rdfs:label xml:lang="en">stir</rdfs:label>
        <skos:definition xml:lang="en">A process that involves the mechanical agitation of a sample, typically in a circular motion.</skos:definition>
        <skos:definition xml:lang="de">Ein Prozess, bei dem eine eine Probe mechanisch bewegt wird, typischerweise in einer kreisförmigen Bewegung.</skos:definition>
        <skos:example xml:lang="de">Das Salz wurde unter Rühren aufgelöst.</skos:example>
        <skos:example xml:lang="en">The salt was dissolved under stirring.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000013 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000013">
        <obo:IAO_0000119>Adopted from ISO 1213-1:2020</obo:IAO_0000119>
        <rdfs:label xml:lang="de">Mischen</rdfs:label>
        <rdfs:label xml:lang="en">mix</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/CHMO_0001685"/>
        <skos:definition xml:lang="de">Die Kombination von zwei oder mehr Materialien mit unterschiedlichen Eigenschaften.</skos:definition>
        <skos:definition xml:lang="en">The combination of two or more materials of different characteristics.</skos:definition>
        <skos:example xml:lang="de">Die beiden Lösungen wurden gemischt.</skos:example>
        <skos:example xml:lang="en">The two solutions were mixed.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/wcso/WCSO_0000014 -->

    <owl:Class rdf:about="https://w3id.org/wcso/WCSO_0000014">
        <obo:IAO_0000119 rdf:resource="http://purl.obolibrary.org/obo/CHMO_0001549"/>
        <rdfs:label xml:lang="de">Trocknen</rdfs:label>
        <rdfs:label xml:lang="en">dry</rdfs:label>
